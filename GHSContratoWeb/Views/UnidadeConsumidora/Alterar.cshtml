                                <div role="tabpanel" class="tab-pane fade show active" id="Section1">
                                    <div class="">
                                        <fieldset class="border p-4">
                                            <legend class="float-none w-auto" style="font-size: 1.2em !important;">Padrão de Entrada</legend>
                                            <div class="row">
                                                <div class="col-12 col-md-3 px-0">
                                                    <div class="form-group">
                                                        <label for="CPF">Concessionaria/Contato</label>
                                                        <input type="text" name="CPF" class="form-control form-control-sm" id="CPF" maxlength="100">
                                                    </div>
                                                </div>
                                                <div class="col-12 col-md-4 px-0">
                                                    <div class="form-group">
                                                        <label for="CPF">Apresentou Fatura de Energia?</label>
                                                        <select class="custom-select custom-select-sm mr-sm-2" id="inlineFormCustomSelect">
                                                            <option selected>-Selecione-</option>
                                                            <option value="1">Sim</option>
                                                            <option value="2">Não</option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-12 col-md-2 px-0">
                                                    <div class="form-group">
                                                        <label for="codigoUc">Código da UC</label>
                                                        <input type="text" name="CPF" class="form-control form-control-sm" id="CPF" maxlength="100">
                                                    </div>
                                                </div>
                                                <div class="col-12 col-md-3 px-0">
                                                    <div class="form-group">
                                                        <label for="demandaContratada">Demanda Contratada (KW)</label>
                                                        <input type="text" name="CPF" class="form-control form-control-sm" id="CPF" maxlength="100">
                                                    </div>
                                                </div>
                                                <div class="col-12 col-md-3 px-0">
                                                    <div class="form-group">
                                                        <label for="categoriaFases">Categoria (fases)</label>
                                                        <input type="text" name="CPF" class="form-control form-control-sm" id="CPF" maxlength="100">
                                                    </div>
                                                </div>
                                                <div class="col-12 col-md-3 px-0">
                                                    <div class="form-group">
                                                        <label for="disjuntor">Disjuntor</label>
                                                        <input type="text" name="CPF" class="form-control form-control-sm" id="CPF" maxlength="100">
                                                    </div>
                                                </div>
                                                <div class="col-12 col-md-3 px-0">
                                                    <div class="form-group">
                                                        <label for="bitolaEntrada">Bitola cabo entrada (mm²)</label>
                                                        <input type="text" name="CPF" class="form-control form-control-sm" id="CPF" maxlength="100">
                                                    </div>
                                                </div>
                                                <div class="col-12 col-md-3 px-0">
                                                    <div class="form-group">
                                                        <label for="tipoLigacao">Tipo de ligação</label>
                                                        <input type="text" name="CPF" class="form-control form-control-sm" id="CPF" maxlength="100">
                                                    </div>
                                                </div>
                                                <div class="col-12 col-md-3 px-0">
                                                    <div class="form-group">
                                                        <label for="qdca">Há QDCA?</label>
                                                        <input type="text" name="CPF" class="form-control form-control-sm" id="CPF" maxlength="100">
                                                    </div>
                                                </div>
                                                <div class="col-12 col-md-6 px-0">
                                                    <div class="form-group">
                                                        <label for="distanciaBitola">Distancia (m) e Bitola (mm²) Padrão até QDCA</label>
                                                        <input type="text" name="CPF" class="form-control form-control-sm" id="CPF" maxlength="100">
                                                    </div>
                                                </div>
                                                <div class="col-12 col-md-3 px-0">
                                                    <div class="form-group">
                                                        <label for="padraoEntrada">Padrão de entrada</label>
                                                        <input type="text" name="CPF" class="form-control form-control-sm" id="CPF" maxlength="100">
                                                    </div>
                                                </div>
